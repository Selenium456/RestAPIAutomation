package EndPoints;

import java.io.IOException;

import Utils.ConfigReader;
import io.restassured.RestAssured;
import io.restassured.http.Method;
import io.restassured.response.Response;
import io.restassured.specification.RequestSpecification;

public class PatientDemoGraphicsEndPoints {
	
	public static ConfigReader configread = null;
	
	
	public PatientDemoGraphicsEndPoints() throws IOException {
		
		configread = new ConfigReader();
		
		
	}
	
	
	public static Response getDemoGraphics() throws IOException {
		
		configread.LoadConfigFile();
		RestAssured.baseURI = configread.getURI();
		
		RequestSpecification request = RestAssured.given();
		
		Response response = request.request(Method.GET,"/demographics");
		
		return response;
		
	}

}
